{
    "name": "onix-solutions/sms-telera",
    "description": "SmsTelera Notifications channel for Laravel 5.3.",
    "keywords": ["laravel", "notifications", "smscentre", "smsc", "СМС-Центр"],
    "homepage": "https://github.com/onix-solutions/sms-telera",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandro Boçon",
            "role": "Developer",
            "email": "contato@onixsolutions.com.br",
            "homepage": "https://github.com/onix-solutions"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "OnixSolutions\\SmsTelera\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnixSolutions\\SmsTelera\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OnixSolutions\\SmsTelera\\SmsTeleraServiceProvider"
            ]
        }
    }
}
